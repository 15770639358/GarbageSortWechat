.photo-view{
  width: 100%;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.takePhoto{
  width: 100rpx;
  height: 100rpx;
  border-radius: 100rpx; 
  background-color: #00cc77;
  color: white;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.cu-modal {
  position: absolute;
  width: 650rpx;
  top: 300rpx;
  left: 50rpx;
  border: 1px rgba(0, 0, 0, 0.5);
  box-shadow: 0 0 20rpx 20rpx rgba(0, 0, 0, 0.3);
  background-color: #fff;
  border-radius: 10rpx;
}

.cu-item {
  margin-left: 30rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
}

.cu-item-list {
  display: flex;
  align-items: center;
}

.close {
  width: 50rpx;
  height: 50rpx;
  font-size: 50rpx;
  position: absolute;
  top: 5rpx;
  right: 5rpx;
}